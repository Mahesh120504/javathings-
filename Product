import java.util.Scanner;
class Product{
    public static void main(String args[]){
        
        int a,b,c;
        Scanner S=new Scanner(System.in);
        System.out.println("enter two numbers");
        a=S.nextInt();
        b=S.nextInt();
        c=a*b;
        System.out.println("Product of the two numbers is "+c);
    }
}
